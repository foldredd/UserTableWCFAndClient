<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<system.serviceModel>
		<services>
			<service name="UserService.UserService" behaviorConfiguration="mexBehavior">
				<endpoint address="UserService.UserService" binding="basicHttpBinding" contract="UserService.IUserService"></endpoint>
				<endpoint address="UserService.UserService" binding="netTcpBinding" contract="UserService.IUserService"></endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"></endpoint>
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8080/"></add>
						<add baseAddress="net.tcp://localhost:45617/"></add>
					</baseAddresses>
				</host>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="mexBehavior">
					<serviceMetadata httpGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>
